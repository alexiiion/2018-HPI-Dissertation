journal = {Proceedings of the National Academy of Sciences},
keywords = {flexure,folding,programmable matter,self-assembly},
mendeley-tags = {flexure,folding,programmable matter,self-assembly},
number = {28},
pages = {12441--12445},
pmid = {20616049},
title = {{Programmable matter by folding}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.0914069107},
