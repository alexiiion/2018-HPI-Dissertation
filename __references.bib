doi = {10.1016/j.jmps.2013.11.014},
file = {:D$\backslash$:/Dropbox/HPI/2018x-PhD-Metamaterials/Related Work/{\_}{\_}{\_}BIBLIOGRAPHY/Overvelde13 - Relating pore shape to the non-linear response of periodic elastomeric structures.pdf:pdf},
