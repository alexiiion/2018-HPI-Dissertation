keywords = {2D structures,3D structures,fabrication,metamaterials,nanoscale},
mendeley-tags = {2D structures,3D structures,fabrication,metamaterials,nanoscale},
number = {March},
pmid = {29573546},
title = {{Nanostructure and Microstructure Fabrication: From Desired Properties to Suitable Processes}},
