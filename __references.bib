journal = {International Journal for Numerical Methods in Engineering},
keywords = {compliant mechanisms,contact-aided compliant mechanisms,non-smooth optimization,topology optimization,understanding,vary cells},
mendeley-tags = {compliant mechanisms,topology optimization,understanding,vary cells},
number = {12},
