isbn = {978-1-4503-4124-0},
keywords = {3D printing,HCI,Non-visual interfaces,accessibility,blind,computer vision.,crowdsourcing,fabrication,visually impaired},
mendeley-tags = {HCI,fabrication},
pages = {315--316},
shorttitle = {ASSETS '16},
title = {{Facade: Auto-generating Tactile Interfaces to Appliances}},
