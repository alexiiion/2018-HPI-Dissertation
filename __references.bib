file = {:D$\backslash$:/Dropbox/HPI/2018x-PhD-Metamaterials/Related Work/{\_}{\_}{\_}BIBLIOGRAPHY/Meza, Greer15 - Resilient 3D hierarchical architected metamaterials.pdf:pdf},
isbn = {0104923520},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {hierarchical,lightweight,material science,metamaterials},
