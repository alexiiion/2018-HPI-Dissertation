journal = {Journal of the Mechanics and Physics of Solids},
keywords = {2D structures,Elasticity,Macroscopic instability,Microscopic instability,Porous structures,experiment,metamaterials},
mendeley-tags = {2D structures,experiment,metamaterials},
month = {mar},
number = {1},
