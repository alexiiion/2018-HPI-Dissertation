}
@inproceedings{Dementyev2016,
abstract = {We introduce Rovables, a miniature robot that can move freely on unmodified clothing. The robots are held in place by mag- netic wheels, and can climb vertically. The robots are un- tethered and have an onboard battery, microcontroller, and wireless communications. They also contain a low-power lo- calization system that uses wheel encoders and IMU, allowing Rovables to perform limited autonomous navigation on the body. In the technical evaluations, we found that Rovables can operate continuously for 45 minutes and can carry up to 1.5N. We propose an interaction space for mobile on-body devices spanning sensing, actuation, and interfaces, and de- velop application scenarios in that space. Our applications include on-body sensing, modular displays, tactile feedback and interactive clothing and jewelry.},
