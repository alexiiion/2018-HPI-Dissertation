file = {:D$\backslash$:/Dropbox/HPI/2018x-PhD-Metamaterials/Related Work/{\_}{\_}{\_}BIBLIOGRAPHY/Gilmore97 - Four Faces of Mass Customization.pdf:pdf},
isbn = {00178012},
