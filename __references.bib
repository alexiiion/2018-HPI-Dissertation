issn = {15261719},
journal = {Journal of Graph Algorithms and Applications},
keywords = {MEMS,graph,shear,theory},
mendeley-tags = {MEMS,graph,shear,theory},
number = {1},
pages = {259--276},
title = {{Planar embeddings of graphs with specified edge lengths}},
url = {papers://92fd5174-9e31-4983-9b1c-5a79350234b8/Paper/p16909},
