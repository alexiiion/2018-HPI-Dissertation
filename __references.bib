file = {:D$\backslash$:/Dropbox/HPI/2018x-PhD-Metamaterials/Related Work/{\_}{\_}{\_}BIBLIOGRAPHY/Sigmund94 - Materials with prescribed constitutive parameters-an inverse homogenization problem.pdf:pdf},
isbn = {0020-7683},
