isbn = {978-1-62410-013-0},
issn = {02734508},
journal = {Engineering},
number = {April},
title = {{Novel Multistable Corrugated Structures}},
url = {http://www2.eng.cam.ac.uk/{~}sdg/preprint/SDM{\_}corrugated.pdf},
