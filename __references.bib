doi = {10.1103/PhysRevE.90.023204},
file = {:D$\backslash$:/Dropbox/HPI/2018x-PhD-Metamaterials/Related Work/{\_}{\_}{\_}BIBLIOGRAPHY/Nadkarni, Kochmann14 - Bistable structures ... wave propagation.pdf:pdf},
