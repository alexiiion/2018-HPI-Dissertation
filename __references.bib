title = {{Comparative study of auxetic geometries by means of computer-aided design and engineering}},
url = {http://stacks.iop.org/0964-1726/21/i=10/a=105004?key=crossref.c6f7c02eff89e04c196afb61933a2d7e},
volume = {21},
